{
  "name": "vue",
  "version": "1.0.0",
  "description": "my vue app",
  "author": "lisper",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "element-ui": "^2.2.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "scripts": {
    "dev-watch": "watchify -vd -e src/app.js -o web/js/app.js",
    "dev-serve": "live-server web --entry-file=index.html",
    "dev": "npm-run-all -p dev-watch dev-serve",
    "p-fonts": "mkdir -p web/css && cp -r static/css/fonts web/css",
    "gen-theme-css": "cat src/components/theme-main/*.css > /tmp/theme-main.css",
    "p-min-css": "cat static/css/normalize.css static/css/main.css /tmp/theme-main.css | csso -o web/css/main.css",
    "p-css": "npm-run-all -s p-fonts gen-theme-css p-min-css",
    "p-html": "html-minifier -c ./.html-minifier.conf static/index.html -o web/index.html",
    "p-image": "cp -r static/image web",
    "p-other": "cp static/other/* web",
    "p-static": "npm-run-all p-html p-css p-image p-other",
    "p-js": "NODE_ENV=production browserify -g envify -e src/app.js | uglifyjs -c -m > web/js/app.js",
    "p": "npm-run-all -s p-js p-static",
    "gp-move": "tar -czf /tmp/web.tar.gz web && tar -xzf /tmp/web.tar.gz -C /home/mhf/js/src/mhf-air.github.io --strip-components 1",
    "gp-git-push": "cd /home/mhf/js/src/mhf-air.github.io && cp index.html 404.html && git add . && git diff-index --quiet HEAD || git commit -m \"fix:\" && git push",
    "gp": "npm-run-all -s p gp-move gp-git-push"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^16.0.0",
    "csso-cli": "^1.1.0",
    "envify": "^4.1.0",
    "html-minifier": "^3.5.9",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "pug": "^2.0.0",
    "stylus": "^0.54.5",
    "uglify-js": "^3.3.10",
    "vueify": "^9.4.1",
    "watchify": "^3.10.0"
  }
}
